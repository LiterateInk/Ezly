{
  "name": "ezly",
  "version": "1.1.0",
  "description": "An easy API wrapper for Izly.",
  "repository": "https://github.com/LiterateInk/Ezly",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "checks": "tsc && eslint ."
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "izly",
    "crous",
    "wrapper",
    "api"
  ],
  "author": "LiterateInk <contact@literate.ink> (https://literate.ink)",
  "homepage": "https://docs.literate.ink/ezly",
  "bugs": {
    "url": "https://github.com/LiterateInk/Ezly/issues"
  },
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.7.2",
    "@types/node": "^22.5.1",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/parser": "^8.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "qrcode": "^1.5.4",
    "terser": "^5.31.6",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@literate.ink/utilities": "1.0.0-10641118381.1",
    "@noble/curves": "^1.6.0",
    "@noble/hashes": "^1.5.0",
    "@peculiar/asn1-ecc": "^2.3.14",
    "@peculiar/asn1-pkcs8": "^2.3.13",
    "@peculiar/asn1-schema": "^2.3.13",
    "@scure/base": "^1.1.8",
    "fast-xml-parser": "^4.4.1"
  }
}
